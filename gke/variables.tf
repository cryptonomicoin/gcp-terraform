variable "project" {
  description = "Value of GCP Region"
  type        = string
  default     = ""
}

variable "region" {
  description = "Value of the GCP Region"
  type        = string
  default     = "us-central1"
}

variable "zone" {
  description = "Value of the GCP Zone"
  type        = string
  default     = "us-central1-c"
}